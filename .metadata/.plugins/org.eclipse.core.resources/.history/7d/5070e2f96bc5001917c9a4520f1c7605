package com.biz.var;

public class Float_Int_01 {
	public static void main(String[] args) {
		float fNum=0;
		int intNum=0;
		long longNum=0;
		
		fNum=intNum;
		// intNum=longNum; // Type Mismatch 발생. 작은 개념에 큰 개념을 넣을 수 없다
		// intNum=fNum; // Type Mismatch 발생. 작은 개념에 큰 개념을 넣을 수 없다
		intNum=(int)fNum; // 소수점 이하 값을 포기하고 형변환하여 넣어라
		longNum=intNum;
		System.out.println(fNum);
	}
}
